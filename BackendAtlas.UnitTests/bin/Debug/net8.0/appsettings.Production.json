{
    "ConnectionStrings": {
        "DefaultConnection": ""
    },
    "Serilog": {
        "MinimumLevel": {
            "Default": "Warning",
            "Override": {
                "Microsoft": "Error",
                "Microsoft.AspNetCore": "Error",
                "Microsoft.EntityFrameworkCore": "Error"
            }
        },
        "WriteTo": [
            {
                "Name": "Console"
            },
            {
                "Name": "File",
                "Args": {
                    "path": "/var/log/backendatlas/app-.log",
                    "rollingInterval": "Day",
                    "retainedFileCountLimit": 90
                }
            }
        ]
    },
    "JwtSettings": {
        "SecretKey": "",
        "Issuer": "BackendAtlas-Production",
        "Audience": "BackendAtlasUsers-Production"
    },
    "CorsOrigins": "https://atlas.tudominio.com,https://www.atlas.tudominio.com"
}